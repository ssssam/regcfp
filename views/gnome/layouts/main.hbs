<!doctype HTML>
<html>
    <head >
        <title>{{ config.theming.site_name }}</title>
        <link rel='stylesheet' type='text/css' href='https://static.gnome.org/css/grass-theme-style.css'/>
        <link rel='shortcut icon' href='https://static.gnome.org/img/logo/foot-16.png'/>

        <meta http-equiv="X-UA-Compatible" content="IE=Edge">
        <link href="https://code.jquery.com/ui/1.10.4/themes/ui-lightness/jquery-ui.min.css" rel="stylesheet">
        <script type="text/javascript" src="/javascripts/jquery-2.1.4.min.js"></script>
        <script type="text/javascript" src="/javascripts/jquery-ui.min.js"></script>
        {{#each extra_js}}
            <script type="text/javascript" src="{{this}}"></script>
        {{/each}}

        <script type="text/javascript">
            // Inserted variables
            {{#if session.currentUser}}
                var currentUser = '{{session.currentUser}}';
            {{else}}
                var currentUser = null;
            {{/if}}

            // Login arguments
            var login_args = {
              siteName: "{{ config.theming.sitename }}",
              siteLogo: "{{ config.theming.logo }}"
            };
        </script>
        <script type="text/javascript" src="/javascripts/reg-fee-slider.js"></script>
        <link rel="stylesheet" href="/css/style.css" type="text/css" media="all">
        <link rel="stylesheet" href="/css/style-gnome.css" type="text/css" media="all">
    </head>


<body>
<div id="global_domain_bar">
<div class="maxwidth">
	<div class="tab">
	<a class="root" href="https://gnome.org/">GNOME.org</a>
	</div>
</div>
</div>


<!-- header -->
    <div id="header" class="container_12">
      <div id="logo" class="grid_3">
        <a title="Go to home page" href="https://guadec.org/"><img style="width: 176px; height: 53px;" src="https://2016.guadec.org/wp-content/uploads/2016/03/guadec-logo-black-1.png" alt="GUADEC: The GNOME Conference" /></a>
      </div>

      <div id="top_bar" class="grid_9">
        <div class="left">
          <div class="menu-globalnav-container">
            <ul id="menu-globalnav" class="menu">
              <li id="menu-item-1039" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-1039">
              <a href="/">Registration Home</a></li>

              <li class="menu-item menu-item-type-post_type menu-item-object-page">
                {{#if session.currentUser}}
                    <a href="/#logout" onclick="{{ login_buttons.logout.onclick }}">Log out</a>
                {{else}}
                    <a href="/#login" onclick="{{ login_buttons.login.onclick }}">Log in</a>
                {{/if}}
              </li>

            </ul>
          </div>
        </div>
      </div>
      
    </div>

    <div class="clearfix"></div>

<div class="container_12">

        {{#if development}}
            <span style="border: 1px solid blue; color:red;" align="center">
            <h1>DEVELOPMENT INSTANCE</h1>
            <h2>Data in this instance may be deleted at any time!</h1>
            </span>
        {{/if}}

		<div class="crafted_content">

        <noscript>
            <p style="color: red;">
                You appear to have JavaScript disabled in your browser. The
                {{ config.theming.site_name }} site requires Javascript to be
                enabled.
            </p>
        </noscript>

        {{{ body }}}

                                    <div id="footer_art" class="grid_12 default">
                            &nbsp;
            </div>

        </div>
</div>

    <div class="clearfix"></div>
    
        <!-- footer grass -->
    <div id="footer_grass">
        &nbsp;
    </div>


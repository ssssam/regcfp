<!doctype HTML>
<html>
    <head>
        <title>{{ config.theming.site_name }}</title>
        <link rel='shortcut icon' href='https://static.gnome.org/img/logo/foot-16.png'/>

        <meta http-equiv="X-UA-Compatible" content="IE=Edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link href="https://code.jquery.com/ui/1.10.4/themes/ui-lightness/jquery-ui.min.css" rel="stylesheet">
        <script type="text/javascript" src="/javascripts/jquery-2.1.4.min.js"></script>
        <script type="text/javascript" src="/javascripts/jquery-ui.min.js"></script>
        {{#each extra_js}}
            <script type="text/javascript" src="{{this}}"></script>
        {{/each}}

        <script type="text/javascript">
            // Inserted variables
            {{#if session.currentUser}}
                var currentUser = '{{session.currentUser}}';
            {{else}}
                var currentUser = null;
            {{/if}}

            // Login arguments
            var login_args = {
              siteName: "{{ config.theming.sitename }}",
              siteLogo: "{{ config.theming.logo }}"
            };
        </script>
        <script type="text/javascript" src="/javascripts/reg-fee-slider.js"></script>
        <script type="text/javascript" src="/javascripts/reg-total.js"></script>

        <link rel='stylesheet' id='sensiblewp-open-sans-css'  href='//fonts.googleapis.com/css?family=Open+Sans%3A400%2C300%2C400italic%2C600%2C700&#038;ver=4.7.2' type='text/css' media='all' />
        <link rel='stylesheet' id='sensiblewp-open-body-css'  href='//fonts.googleapis.com/css?family=Open+Sans%3A400%2C300%2C400italic%2C600%2C700&#038;ver=4.7.2' type='text/css' media='all' />
        <link rel="stylesheet" href="/css/font-awesome.css" type="text/css" media="all">

        <link rel="stylesheet" href="https://2017.guadec.org/wp-content/themes/sensible-wp/style.css?ver=4.7.2" type="text/css" media="all" />
        <link rel="stylesheet" href="https://2017.guadec.org/?sccss=1&ver=4.7.2" type="text/css" media="all" />
        <link rel="stylesheet" href="/css/style.css" type="text/css" media="all" />
        <link rel="stylesheet" href="/css/style-gnome.css" type="text/css" media="all" />
        <link rel="stylesheet" href="/css/960.css" type="text/css" media="all" />
        <link rel="stylesheet" href="/css/960-responsive.css" type="text/css" media="all" />
    </head>

<body>
  <header id="masthead" class="site-header" role="banner">
    <div class="grid grid-pad head-overflow">
      <div class="navigation-container">
        <nav id="site-navigation" class="main-navigation" role="navigation">
          <div class="menu-main-container">
            <ul id="menu-main" class="menu">
              <li class="menu-item menu-item-type-custom menu-item-object-custom menu-item-home">
                <a href="https://2017.guadec.org/">GUADEC</a>
              </li>

              <li class="menu-item menu-item-type-custom menu-item-object-custom menu-item-home">
                <a href="/">Registration Home</a>
              </li>

              <li class="menu-item menu-item-type-custom menu-item-object-custom menu-item-home">
                <form>
                  {{#if session.currentUser}}
                    <button {{{ login_buttons.logout }}}>Log out</a>
                  {{else}}
                    <button {{{ login_buttons.login }}}>Log in</a>
                   {{/if}}
                </form>
              </li>
            </ul>
          </div>
        </nav>
      </div>
    </div>
  </header>

  <section id="content" class="site-content">

    <header class="entry-header">
      <div class="grid grid-pad">
        <div class="col-1-1">
          <h1 class="entry-title">Registration</h1>
      </div>
    </div>
   </header>
   <div class="grid grid-pad">

     <div class="container_12">

        {{#if development}}
          <h3 style="color: red;">DEVELOPMENT INSTANCE!</h3>
        {{/if}}

		<div class="crafted_content">

        <noscript>
            <p style="color: red;">
                You appear to have JavaScript disabled in your browser. The
                {{ config.theming.site_name }} site requires Javascript to be
                enabled.
            </p>
        </noscript>

        {{#if error}}
          <div class="validation">Something went wrong: {{error}}.</div>
        {{/if}}

        {{{ body }}}

        </div>
      </div>
    </div>
  </section>

    <!-- footer -->
    <div id="footer">
        <div class="container_12">
            <div class="links grid_9">
                <div class="menu-footer-container"><ul id="menu-footer" class="menu"><li id="menu-item-1048" class="menu-item menu-item-type-custom menu-item-object-custom current-menu-item current_page_item menu-item-has-children menu-item-1048"><a href="/">The GNOME Project</a>
<ul class="sub-menu">
